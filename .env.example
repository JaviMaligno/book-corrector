# Google Gemini API configuration
# Get your API key from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_api_key_here
GEMINI_MODEL=gemini-2.5-flash

# Integration testing
RUN_GEMINI_INTEGRATION=0

# Authentication tokens (for testing)
TOKEN=your_jwt_token_here
TEST_EMAIL=demo@example.com
TEST_PASSWORD=demo123

# Azure OpenAI configuration (optional)
AZURE_OPENAI_ENDPOINT=https://your-resource.cognitiveservices.azure.com
AZURE_OPENAI_API_KEY=your_azure_api_key_here
AZURE_OPENAI_DEPLOYMENT_NAME=gpt-4
AZURE_OPENAI_API_VERSION=2025-04-01-preview
AZURE_OPENAI_MODEL_NAME=gpt-4

# Azure OpenAI fallback (optional)
AZURE_OPENAI_FALLBACK_DEPLOYMENT_NAME=gpt-4.1
AZURE_OPENAI_FALLBACK_API_VERSION=2025-01-01-preview

# Server runtime configuration
DEMO_PLAN=free
SYSTEM_MAX_WORKERS=2
LOG_LEVEL=info

# Database (future usage)
DATABASE_URL=sqlite:///./local.db

